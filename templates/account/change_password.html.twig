{% extends 'base.html.twig' %}

{% block title %} 'Change password' {% endblock %}

{% block body %}
<div class="row">
  <div class="col-md-6 mx-auto">
    <h1 class="my-5 text-center"> Change password </h1>
      {{form_start(form)}}
        {{form_widget(form)}}
        <button type="submit" name="button" class="btn btn-primary btn-lg btn-block"> Apply changes</button>
      {{form_end(form)}}
  </div>
</div>
{% endblock %}
